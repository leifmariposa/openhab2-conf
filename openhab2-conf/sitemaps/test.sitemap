sitemap test label="Mostigen 1"
{
  //String LINK_THERMOSTAT "<a href='http://leifmariposa.asuscomm.com:3000/d/vgY0H8ZRz/temperature-dashboard?orgId=1'>Thermostat control</a>"
  Frame label="Who's home" icon="group" {
    Text item=LeifsPhone label="Leif"
    Text item=KellysPhone label="Kelly"
    Text item=AlexandersPhone label="Alexander"
    Text item=JonathansPhone label="Jonathan"
  }

  Frame label="Lights" {
    Switch item=turn_on_all_lights mappings=[ON="Activate"]
    Switch item=turn_off_all_lights mappings=[ON="Activate"]
    Text label="More lights ..." {
      Switch item=EntranceWallLight label="Entrance"
      Switch item=HallwayLight label="Hallway"
      Switch item=LivingRoomBureauLight label="Living room, bureau"
      Switch item=LivingRoomWindow1Light label="Living room, window 1"
      Switch item=LivingRoomWindow2Light label="Living room, window 2"
      Slider item=Livingroom_Brightness label="Living room brightness [%.0f %%]"
      Slider item=Livingroom_Color label="Living room color [%.0f %%]"
      Slider item=Livingroom_Color label="Living room color [%.0f %%]"
      Slider item=LivingRoomLeftBulbBrightness label="Living room left light brightness [%.0f %%]"
      Slider item=LivingRoomRightBulbBrightness label="Living room right light brightness [%.0f %%]"
      Slider item=GardenBulbBrightness label="Garden light brightness [%.0f %%]"
      Slider item=LivingRoomLeftBulbColor label="Living room left light color [%.0f %%]"
      Slider item=LivingRoomRightBulbColor label="Living room right light color [%.0f %%]"
      Slider item=LivingRoomLeftBulbBrightness label="Living room left light brightness [%.0f %%]"
    }
  }

  Frame label="Temperatures" {
    Text item=LivingRoomSensorTemperature label="In"
    Text item=GardenSensorTemperature label="Out"
    Text label="More temperatures ..." {
      Text item=FreezerSensorTemperature label="Freezer"
      Text item=FridgeSensorTemperature label="Fridge"
      Text item=GardenSensorTemperature label="Garden"
      Text item=LivingRoomSensorTemperature label="Living room"
      Text item=KitchenSensorTemperature label="Kitchen"
      Text item=MasterBedroomSensorTemperature label="Master bedroom"
      Text item=ConservatorySensorTemperature label="Conservatory"
    }
    Text label="Temperature graphs" icon="line" {
      Frame label="Time period" {
        Switch item=General_ChartPeriod label="Select" mappings=[0="Hour", 1="Day", 2="Week", 3="Month", 4="Year"]
      }
      Frame label="Garden" {
        Chart item=GardenSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=GardenSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=GardenSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=GardenSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=GardenSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Conservatory" {
        Chart item=ConservatorySensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=ConservatorySensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=ConservatorySensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=ConservatorySensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=ConservatorySensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Living room" {
        Chart item=LivingRoomSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=LivingRoomSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=LivingRoomSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=LivingRoomSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=LivingRoomSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Kitchen" {
        Chart item=KitchenSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=KitchenSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=KitchenSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=KitchenSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=KitchenSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Master bedroom" {
        Chart item=MasterBedroomSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=MasterBedroomSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=MasterBedroomSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=MasterBedroomSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=MasterBedroomSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Refrigerator" {
        Chart item=FridgeSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=FridgeSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=FridgeSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=FridgeSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=FridgeSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
      Frame label="Freezer" {
        Chart item=FreezerSensorTemperature period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=FreezerSensorTemperature period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=FreezerSensorTemperature period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=FreezerSensorTemperature period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=FreezerSensorTemperature period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
    }
  }

  Frame label="Electricity" {
    Text item=eletricity_consumption
    Text item=kwh_today
    Text item=energy_cost_today
    Text label="More electricity ..." {
      Text item=eletricity_consumption
      Text item=kwh_today
      Text item=kwh_yesterday
      Text item=kwh_two_days_ago
      Text item=kwh_three_days_ago
      Text item=kwh_four_days_ago
      Text item=kwh_five_days_ago
      Text item=kwh_month
      Text item=kwh_last_month
      Text item=kwh_current
      Text item=kwh_at_midnight
      Text item=kwh_at_new_month
    }
    Text label="Graph" icon="line" {
      Frame label="Electricity consumption" {
        Switch item=General_ChartPeriod label="Time period" mappings=[0="Hour", 1="Day", 2="Week", 3="Month", 4="Year"]
        Chart item=eletricity_consumption period=h refresh=1000 visibility=[General_ChartPeriod==0]
        Chart item=eletricity_consumption period=d refresh=1000 visibility=[General_ChartPeriod==1, General_ChartPeriod=="NULL"]
        Chart item=eletricity_consumption period=W refresh=1000 visibility=[General_ChartPeriod==2]
        Chart item=eletricity_consumption period=M refresh=1000 visibility=[General_ChartPeriod==3]
        Chart item=eletricity_consumption period=Y refresh=1000 visibility=[General_ChartPeriod==4]
      }
    }
  }

  Frame label="Alarm" {
    Text item=AlarmStatus label="Status"
    Text item=AlarmStatusTimeChanged label="Time changed"
    Switch item=EntranceDoorSensor
  }

  /*Frame label="Neato BotVac Connected" {
    Switch item=NeatoCommand mappings=[clean="Clean",stop="Stop",pause="Pause",resume="Resume", dock="Send to dock"]
    Text item=NeatoBattery label="Battery level"
    Text item=NeatoState
    Text item=NeatoError label="Error Message" icon="siren"
    Text item=NeatoAction label="Activity"
    Text item=NeatoIsDocked label="In dock"
    Group label="Mer information" item=GroupNeato
  }*/

  Frame label="Misc" {
    Switch item=reboot_pi
    //Switch item=MQTT_Test
  }

  /*Frame label="MQTT" {
    Switch item=tv_power_on mappings=[ON="Activate"]
    Switch item=tv_power_off mappings=[ON="Activate"]
    Switch item=tv_volume_up mappings=[ON="Activate"]
    Switch item=tv_volume_down mappings=[ON="Activate"]
    Switch item=lg_power mappings=[ON="Activate"]
    Switch item=lg_volume_up mappings=[ON="Activate"]
    Switch item=lg_volume_down mappings=[ON="Activate"]
  }*/
}
